{
    "env": {
        "browser": true,
        "es2020": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": ["off",{"vars": "local"}]
    },
    "globals": {
        "includes" : "writable",
        "include" : "writable",
        "MENU" : "writable"
    }
}
